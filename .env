# Database Configuration
DB_TYPE=postgresdb
DB_POSTGRESDB_HOST=127.0.0.1
DB_POSTGRESDB_PORT=5432
DB_POSTGRESDB_DATABASE=n8n
DB_POSTGRESDB_USER=n8n
DB_POSTGRESDB_PASSWORD=eIq9vN70zuaDHY

# N8N Core Configuration
N8N_SKIP_LICENSE_CHECK=true
N8N_DATABASE_MIGRATE=true
GENERIC_TIMEZONE=America/Sao_Paulo

# Authentication
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=GkHujeNsxBF39V

# Multi-tenant and Runners
N8N_MULTI_TENANT_ENABLED=true
N8N_MULTI_TENANT_SUBDOMAIN_ENABLED=false
N8N_RUNNERS_ENABLED=true

# URLs e Webhook (ajuste conforme necess√°rio)
N8N_HOST=localhost
N8N_PORT=5678
N8N_PROTOCOL=http
WEBHOOK_URL=http://localhost:5678/
N8N_EDITOR_BASE_URL=http://localhost:5678

# Security
N8N_SECURE_COOKIE=false
N8N_JWT_AUTH_HEADER=authorization
N8N_JWT_AUTH_HEADER_VALUE_PREFIX=Bearer

# Logging
N8N_LOG_LEVEL=info
N8N_LOG_OUTPUT=console

# Execution Settings
EXECUTIONS_PROCESS=main
EXECUTIONS_MODE=regular
EXECUTIONS_TIMEOUT=3600
EXECUTIONS_MAX_TIMEOUT=3600
EXECUTIONS_DATA_SAVE_ON_ERROR=all
EXECUTIONS_DATA_SAVE_ON_SUCCESS=all
EXECUTIONS_DATA_SAVE_ON_PROGRESS=false
EXECUTIONS_DATA_SAVE_MANUAL_EXECUTIONS=true

# Queue Settings (Redis)
QUEUE_BULL_REDIS_HOST=redis
QUEUE_BULL_REDIS_PORT=6379
QUEUE_BULL_REDIS_DB=0

# Email Configuration (optional - configure if needed)
N8N_EMAIL_MODE=smtp
N8N_SMTP_HOST=smtp.gmail.com
N8N_SMTP_PORT=587
N8N_SMTP_SSL=false
N8N_SMTP_USER=your-email@gmail.com
N8N_SMTP_PASS=your-app-password
N8N_SMTP_SENDER=your-email@gmail.com

# Metrics (optional)
N8N_METRICS=false
N8N_METRICS_PREFIX=n8n_

# File Upload
N8N_DEFAULT_BINARY_DATA_MODE=filesystem
N8N_BINARY_DATA_TTL=1440
