#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/projects/n8n-super/node_modules/.pnpm/ts-jest@29.1.1_@babel+core@7.26.10_@jest+types@29.6.1_babel-jest@29.6.2_@babel+core@7.2_9b1edcdbf0ebbe30d78ef14cb5a0f849/node_modules/ts-jest/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/projects/n8n-super/node_modules/.pnpm/ts-jest@29.1.1_@babel+core@7.26.10_@jest+types@29.6.1_babel-jest@29.6.2_@babel+core@7.2_9b1edcdbf0ebbe30d78ef14cb5a0f849/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/projects/n8n-super/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/projects/n8n-super/node_modules/.pnpm/ts-jest@29.1.1_@babel+core@7.26.10_@jest+types@29.6.1_babel-jest@29.6.2_@babel+core@7.2_9b1edcdbf0ebbe30d78ef14cb5a0f849/node_modules/ts-jest/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/projects/n8n-super/node_modules/.pnpm/ts-jest@29.1.1_@babel+core@7.26.10_@jest+types@29.6.1_babel-jest@29.6.2_@babel+core@7.2_9b1edcdbf0ebbe30d78ef14cb5a0f849/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/projects/n8n-super/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../ts-jest/cli.js" "$@"
fi
