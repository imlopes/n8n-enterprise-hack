#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/projects/n8n-super/node_modules/.pnpm/@n8n+vm2@3.9.25/node_modules/@n8n/vm2/bin/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/projects/n8n-super/node_modules/.pnpm/@n8n+vm2@3.9.25/node_modules/@n8n/vm2/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/projects/n8n-super/node_modules/.pnpm/@n8n+vm2@3.9.25/node_modules/@n8n/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/projects/n8n-super/node_modules/.pnpm/@n8n+vm2@3.9.25/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/projects/n8n-super/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/projects/n8n-super/node_modules/.pnpm/@n8n+vm2@3.9.25/node_modules/@n8n/vm2/bin/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/projects/n8n-super/node_modules/.pnpm/@n8n+vm2@3.9.25/node_modules/@n8n/vm2/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/projects/n8n-super/node_modules/.pnpm/@n8n+vm2@3.9.25/node_modules/@n8n/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/projects/n8n-super/node_modules/.pnpm/@n8n+vm2@3.9.25/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/projects/n8n-super/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@n8n/vm2/bin/vm2" "$@"
else
  exec node  "$basedir/../@n8n/vm2/bin/vm2" "$@"
fi
