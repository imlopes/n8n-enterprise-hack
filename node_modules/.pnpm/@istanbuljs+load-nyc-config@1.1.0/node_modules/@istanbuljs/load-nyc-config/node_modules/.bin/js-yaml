#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/projects/n8n-super/node_modules/.pnpm/js-yaml@3.14.1/node_modules/js-yaml/bin/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/projects/n8n-super/node_modules/.pnpm/js-yaml@3.14.1/node_modules/js-yaml/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/projects/n8n-super/node_modules/.pnpm/js-yaml@3.14.1/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/projects/n8n-super/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/projects/n8n-super/node_modules/.pnpm/js-yaml@3.14.1/node_modules/js-yaml/bin/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/projects/n8n-super/node_modules/.pnpm/js-yaml@3.14.1/node_modules/js-yaml/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/projects/n8n-super/node_modules/.pnpm/js-yaml@3.14.1/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/projects/n8n-super/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../js-yaml@3.14.1/node_modules/js-yaml/bin/js-yaml.js" "$@"
else
  exec node  "$basedir/../../../../../../js-yaml@3.14.1/node_modules/js-yaml/bin/js-yaml.js" "$@"
fi
