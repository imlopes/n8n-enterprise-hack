#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/projects/n8n-super/packages/core/bin/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/projects/n8n-super/packages/core/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/projects/n8n-super/packages/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/projects/n8n-super/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/projects/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/tools/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/node_modules:/Users/ilo/projects/02-odoo/node_modules:/Users/ilo/projects/node_modules:/Users/ilo/node_modules:/Users/node_modules:/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/projects/n8n-super/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/projects/n8n-super/packages/core/bin/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/projects/n8n-super/packages/core/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/projects/n8n-super/packages/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/projects/n8n-super/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/projects/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/tools/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/node_modules:/Users/ilo/projects/02-odoo/node_modules:/Users/ilo/projects/node_modules:/Users/ilo/node_modules:/Users/node_modules:/node_modules:/Users/ilo/projects/02-odoo/04-nomadi/tools/unraid/developer-tools/compose-manager/projects/n8n-super/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../n8n-core/bin/copy-static-files" "$@"
else
  exec node  "$basedir/../n8n-core/bin/copy-static-files" "$@"
fi
